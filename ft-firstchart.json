    "subtitleFontStyle": "italic",
    "subtitleFontSize": 11,
    "anchor": "start",
    "color": "black"
  },
  "data": {
    "url": "https://raw.githubusercontent.com/RRB2023/RRB2023.github.io/main/IMFcreditoutstandingdataclean.csv"
